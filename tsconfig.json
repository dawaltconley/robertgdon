{
  "include": ["src/**/*", "client-directives/client-directives.d.ts"],
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "~/*": ["*"],
      "@tina/*": ["tina/*"],
      "@components/*": ["src/components/*"],
      "@layouts/*": ["src/layouts/*"],
      "@lib/*": ["src/lib/*"],
      "@browser/*": ["src/lib/browser/*"],
      "@build/*": ["src/lib/node/*"],
      "@hooks/*": ["src/lib/hooks/*"],
      "@styles/*": ["src/styles/*"]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}
